{
  "name": "next-ts",
  "description": "A Typescript powered NextJS kit with Chakra UI and Recoil",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "husky:install": "husky install",
    "husky:init": "npx husky-init && yarn",
    "pre-commit": "lint-staged",
    "prettier:check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "yarn lint:es && yarn lint:styled",
    "lint:es": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src/",
    "lint:es-fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:styled": "stylelint ./src/**/*.{css,scss,js,ts,tsx}",
    "lint:styled-fix": "stylelint ./src/**/*.{css,scss,js,ts,tsx} --fix",
    "storybook": "start-storybook --no-open  -p 6006",
    "build:storybook": "build-storybook",
    "sitemap": "next-sitemap",
    "post-install": "yarn run theme-typings",
    "theme-typings": "chakra-cli tokens './theme/index.ts'",
    "cy:test-dev": "start-server-and-test 'yarn build && yarn start' 3000 'yarn cy:run-dev'",
    "cy:run-dev": "cypress run --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run",
    "cy:open-e2e": "cypress open --e2e",
    "test:jest": "jest --passWithNoTests",
    "test:jest-since-dev": "jest --changedSince=develop",
    "test:jest-watch": "jest --watch",
    "test:prep": "yarn build && yarn start -p 3099",
    "test:e2e": "NODE_ENV=test start-server-and-test 'yarn start -p 3099' 3099 'yarn cy:open'",
    "merge-reports": "mkdir -p mochareports && mochawesome-merge --reportDir cypress/results >> mochareports/mochawesome.json",
    "create-report": "marge mochareports/mochawesome.json -f report -o mochareports",
    "browserslist:update": "npx browserslist@latest --update-db",
    "pkg:add": "sh ./scripts/yarn-add.sh",
    "pkg:del": "sh ./scripts/yarn-del.sh",
    "pkg:install": "sh ./scripts/yarn-smart-install.sh"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.1",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "deep-extend": "^0.6.0",
    "framer-motion": "^6.3.11",
    "fuse.js": "^6.6.2",
    "i18next": "^21.8.10",
    "joi": "^17.6.0",
    "next": "12.1.6",
    "next-i18next": "^11.0.0",
    "next-seo": "^5.4.0",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.32.2",
    "react-i18next": "^11.17.2",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-query": "^3.39.1",
    "recoil": "^0.7.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.3",
    "@babel/core": "^7.18.5",
    "@chakra-ui/cli": "^2.1.0",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@emotion/babel-plugin": "^11.9.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/deep-extend": "^0.4.32",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.0",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__cypress": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.1.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^7.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-runner-eslint": "^1.0.1",
    "lint-staged": "^13.0.2",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-sitemap": "^3.1.1",
    "postcss": "^8.4.14",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "storybook-addon-performance": "^0.16.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "~9.3.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.73.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": "true"
  }
}
