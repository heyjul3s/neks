    "next-compose-plugins": "^2.2.1",
    "next-sitemap": "^1.6.192",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "~8.2.0",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": "true"
  }
