{
  "name": "neks",
  "description": "A Typescript powered NextJS kit with Chakra UI and Recoil",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sitemap": "next-sitemap",
    "post-install": "yarn run theme-typings",
    "theme-typings": "chakra-cli tokens './theme/index.ts'",
    "cy": "cypress run",
    "cy-open": "cypress open",
    "merge-reports": "mkdir -p mochareports && mochawesome-merge --reportDir cypress/results >> mochareports/mochawesome.json",
    "create-report": "marge mochareports/mochawesome.json -f report -o mochareports",
    "test": "jest --config jest.config.js",
    "test-watch": "jest --config jest.config.js --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@googlemaps/js-api-loader": "^1.12.7",
    "@hookform/error-message": "^2.0.0",
    "axios": "^0.24.0",
    "date-fns": "^2.25.0",
    "deep-extend": "^0.6.0",
    "framer-motion": "^4.1.17",
    "fuse.js": "^6.5.3",
    "i18next": "^21.3.3",
    "next": "11.1.2",
    "next-i18next": "^8.9.0",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.5",
    "react-i18next": "^11.12.0",
    "react-query": "^3.28.0",
    "recoil": "^0.4.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.1",
    "@babel/core": "^7.15.8",
    "@chakra-ui/cli": "^1.5.3",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@emotion/babel-plugin": "^11.3.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/deep-extend": "^0.4.32",
    "@types/gtag.js": "^0.0.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/styled-components": "5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.3.0",
    "cypress": "^8.7.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "eslint": "8.1.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
