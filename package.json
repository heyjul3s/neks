{
  "name": "neks",
  "description": "A Typescript powered NextJS kit with Chakra UI and Recoil",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:es && yarn lint:styled",
    "lint:es": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src/",
    "lint:es-fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:styled": "stylelint ./src/**/*.{css,scss,js,ts,tsx}",
    "lint:styled-fix": "stylelint ./src/**/*.{css,scss,js,ts,tsx} --fix",
    "storybook": "start-storybook --no-open  -p 6006",
    "build-storybook": "build-storybook",
    "sitemap": "next-sitemap",
    "post-install": "yarn run theme-typings",
    "theme-typings": "chakra-cli tokens './theme/index.ts'",
    "cy": "cypress run",
    "cy-open": "cypress open",
    "merge-reports": "mkdir -p mochareports && mochawesome-merge --reportDir cypress/results >> mochareports/mochawesome.json",
    "create-report": "marge mochareports/mochawesome.json -f report -o mochareports",
    "test:jest": "jest --passWithNoTests",
    "test:jest-since-dev": "jest --changedSince=develop",
    "test:jest-watch": "jest --watch"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@googlemaps/js-api-loader": "^1.13.6",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.8",
    "axios": "^0.26.0",
    "date-fns": "^2.28.0",
    "deep-extend": "^0.6.0",
    "framer-motion": "^6.2.6",
    "fuse.js": "^6.5.3",
    "i18next": "^21.6.11",
    "joi": "^17.6.0",
    "next": "12.1.0",
    "next-i18next": "^10.3.0",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.1",
    "react-i18next": "^11.15.4",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-query": "^3.34.16",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.0",
    "@babel/core": "^7.17.5",
    "@chakra-ui/cli": "^1.8.2",
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "@types/deep-extend": "^0.4.32",
    "@types/gtag.js": "^0.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/styled-components": "5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-watch": "^8.0.0",
    "jest": "^27.5.1",
    "jest-runner-eslint": "^1.0.0",
    "mocha": "^9.2.1",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-sitemap": "^2.4.5",
    "prettier": "^2.5.1",
    "storybook-addon-performance": "^0.16.1",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "~9.2.6",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.69.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": "true"
  }
}
